<section>
  <div class="addbottun-position">
    <h1>
      <a href="/">メモアプリ</a>
    </h1>
    <form action="/new" method="GET">
      <button class="add-button" type="submit">メモを作成</button>
    </form>
  </div>
</section>
<section>
  <ul>
    <% @memo_list.each do |id,post| %>
      <li>
        <a href="/memo/<%= id %>"><%= CGI.escapeHTML(post["title"]) %></a>
      </li>
    <% end %>
  </ul>
</section>
